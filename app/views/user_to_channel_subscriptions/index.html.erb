<p id="notice"><%= notice %></p>

<h1>Listing User To Channel Subscriptions</h1>

<table>
  <thead>
    <tr>
      <th>Channel</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_to_channel_subscriptions.each do |user_to_channel_subscription| %>
      <tr>
        <td><%= user_to_channel_subscription.channel %></td>
        <td><%= user_to_channel_subscription.user %></td>
        <td><%= link_to 'Show', user_to_channel_subscription %></td>
        <td><%= link_to 'Edit', edit_user_to_channel_subscription_path(user_to_channel_subscription) %></td>
        <td><%= link_to 'Destroy', user_to_channel_subscription, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User to channel subscription', new_user_to_channel_subscription_path %>
